{
  "watch": [
    "packages/shared/src",
    "packages/homebridge-yeelight/src",
    "packages/yeelight/src"
  ],
  "ext": "js",
  "ignore": [],
  "exec": "DEBUG=-babel,-HAP-NodeJS*,* homebridge -D -P ./packages -U ./homebridge",
  "signal": "SIGTERM",
  "env": {
    "NODE_OPTIONS": "--trace-warnings"
  }
}
